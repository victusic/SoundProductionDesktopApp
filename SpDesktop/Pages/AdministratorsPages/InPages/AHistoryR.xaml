<Page x:Class="SpDesktop.Pages.AdministratorsPages.InPages.AHistoryR"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SpDesktop.Pages.AdministratorsPages.InPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AHistoryR">

    <Grid>
        <DataGrid Name="DGrid" AutoGenerateColumns="False" Margin="15, 15, 15, 15" IsReadOnly="True">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Id пользователя" Width="100" Binding="{Binding UserId}"/>
                <DataGridTextColumn Header="Роль"  Width="200" Binding="{Binding Roll1.Roll1}" />
                <DataGridTextColumn Header="Имя"  Width="*" Binding="{Binding Login}" />
                <DataGridTextColumn Header="Дата и время" Width="200" Binding="{Binding RegistrationDate}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
